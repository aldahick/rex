{
  "name": "@aldahick/rex-api",
  "version": "0.3.0",
  "description": "Personal API server",
  "main": "dist/main.js",
  "type": "module",
  "scripts": {
    "start": "node --enable-source-maps dist/main",
    "dev": "tsc-watch --onSuccess \"node --enable-source-maps dist/main\"",
    "build": "tsc",
    "lint": "biome check src",
    "generate": "graphql-codegen --config codegen.graphql.yml",
    "db": "ts-node --esm src/util/orchid.ts",
    "test": "node --enable-source-maps --test dist"
  },
  "engines": {
    "node": "^20.10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aldahick/rex.git"
  },
  "author": "Alex Hicks <alex@alexhicks.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aldahick/rex/issues"
  },
  "homepage": "https://github.com/aldahick/rex#readme",
  "dependencies": {
    "@athenajs/core": "^1.1.0",
    "@athenajs/utils": "^1.1.0",
    "@google-cloud/run": "^1.0.2",
    "axios": "^1.6.8",
    "bcrypt": "^5.1.1",
    "discord.js": "^14.14.1",
    "dockerode": "^4.0.0",
    "gamedig": "^4.3.0",
    "googleapis": "^129.0.0",
    "graphql": "^16.8.1",
    "jsonwebtoken": "^9.0.2",
    "mime": "^4.0.0",
    "orchid-orm": "^1.18.3",
    "pg": "^8.11.3",
    "pluralize": "^8.0.0",
    "pqb": "^0.20.1",
    "reflect-metadata": "^0.1.14",
    "remeda": "^1.56.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.6.3",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/typescript": "^4.0.6",
    "@graphql-codegen/typescript-operations": "^4.2.0",
    "@types/bcrypt": "^5.0.2",
    "@types/dockerode": "^3.3.23",
    "@types/gamedig": "^4.0.5",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/mime": "^3.0.4",
    "@types/node": "^20.10.4",
    "@types/pg": "^8.10.9",
    "@types/pluralize": "^0.0.33",
    "rake-db": "^2.11.3",
    "ts-node": "^10.9.2",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.3.3"
  }
