    "bcrypt": "^5.1.1",
    "discord.js": "^14.13.0",
    "dockerode": "^3.3.5",
    "gamedig": "^4.1.0",
    "googleapis": "^126.0.1",
    "graphql": "^16.8.1",
    "jsonwebtoken": "^9.0.2",
    "mime": "^3.0.0",
