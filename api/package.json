{
  "name": "@aldahick/rex-api",
  "version": "0.1.9",
  "description": "Personal API server",
  "main": "dist/main.js",
  "type": "module",
  "scripts": {
    "start": "node --enable-source-maps dist/main",
    "dev": "tsc-watch --onSuccess \"node --enable-source-maps dist/main.js\"",
    "build": "tsc",
    "lint": "eslint src",
    "generate": "graphql-codegen --config codegen.graphql.yml",
    "db": "ts-node --esm src/util/orchid.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aldahick/rex.git"
  },
  "author": "Alex Hicks <alex@alexhicks.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aldahick/rex/issues"
  },
  "homepage": "https://github.com/aldahick/rex#readme",
  "dependencies": {
    "@athenajs/core": "^0.0.21",
    "@athenajs/utils": "^0.0.13",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "discord.js": "^14.11.0",
    "dockerode": "^3.3.5",
    "gamedig": "^4.0.6",
    "googleapis": "^118.0.0",
    "graphql": "^16.7.1",
    "jsonwebtoken": "^9.0.0",
    "mime": "^3.0.0",
    "orchid-orm": "^1.13.6",
    "pg": "^8.11.1",
    "pluralize": "^8.0.0",
    "pqb": "^0.16.3",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^4.0.1",
    "@graphql-codegen/typescript": "^4.0.1",
    "@graphql-codegen/typescript-operations": "^4.0.1",
    "@types/bcrypt": "^5.0.0",
    "@types/dockerode": "^3.3.19",
    "@types/gamedig": "^4.0.1",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/mime": "^3.0.1",
    "@types/node": "^20.4.2",
    "@types/pg": "^8.10.2",
    "@types/pluralize": "^0.0.30",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "prettier": "^3.0.0",
    "rake-db": "^2.10.7",
    "ts-node": "^10.9.1",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.1.6"
  },
  "pnpm": {
    "auditConfig": {
      "ignoreCves": [
        "CVE-2022-25883"
      ]
    }
