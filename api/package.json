{
  "name": "@aldahick/rex-api",
  "version": "0.3.0",
  "description": "Personal API server",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "start": "node --enable-source-maps dist/main",
    "dev": "tsc-watch --onSuccess \"node --enable-source-maps dist/index.js\"",
    "build": "tsc",
    "docker:build": "bash scripts/docker-build.sh",
    "lint": "eslint src",
    "generate:graphql": "graphql-codegen --config codegen.graphql.yml",
    "db": "ts-node --esm src/service/database/orchid-cli.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aldahick/rex.git"
  },
  "author": "Alex Hicks <alex@alexhicks.net> (https://alexhicks.net)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aldahick/rex/issues"
  },
  "homepage": "https://github.com/aldahick/rex#readme",
  "dependencies": {
    "@athenajs/core": "^0.0.12",
    "axios": "^1.4.0",
    "bcrypt": "^5.1.0",
    "discord.js": "^14.11.0",
    "gamedig": "^4.0.6",
    "googleapis": "^118.0.0",
    "graphql": "^16.6.0",
    "mime": "^3.0.0",
    "orchid-orm": "^1.10.5",
    "pg": "^8.11.0",
    "pluralize": "^8.0.0",
    "pqb": "^0.12.4",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^4.0.1",
    "@graphql-codegen/typescript": "^4.0.0",
    "@graphql-codegen/typescript-operations": "^4.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/gamedig": "^4.0.1",
    "@types/mime": "^3.0.1",
    "@types/node": "^20.2.5",
    "@types/pg": "^8.10.2",
    "@types/pluralize": "^0.0.29",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "prettier": "^2.8.8",
    "rake-db": "^2.8.44",
    "ts-node": "^10.9.1",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.1.3"
  }
}
