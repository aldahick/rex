{
  "name": "@aldahick/rex-api",
  "version": "0.3.6",
  "description": "Personal API server",
  "main": "dist/main.js",
  "type": "module",
  "scripts": {
    "start": "node --enable-source-maps dist/main",
    "dev": "tsc-watch --onSuccess \"node --enable-source-maps dist/main\"",
    "build": "tsc",
    "lint": "biome check src",
    "generate": "graphql-codegen --config codegen.graphql.ts",
    "db": "tsx src/service/database/database.migration.ts",
    "test": "tsx --enable-source-maps --test src"
  },
  "engines": {
    "node": "^20.13"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aldahick/rex.git"
  },
  "author": "Alex Hicks <alex@alexhicks.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aldahick/rex/issues"
  },
  "homepage": "https://github.com/aldahick/rex#readme",
  "dependencies": {
    "@athenajs/core": "^1.2.0",
    "@google-cloud/run": "^1.2.0",
    "axios": "^1.6.8",
    "bcrypt": "^5.1.1",
    "discord.js": "^14.15.2",
    "dockerode": "^4.0.2",
    "googleapis": "^137.1.0",
    "graphql": "^16.8.1",
    "graphql-request": "6.1.0",
    "jsonwebtoken": "^9.0.2",
    "mime": "^4.0.3",
    "orchid-orm": "^1.27.4",
    "pg": "^8.11.5",
    "pluralize": "^8.0.0",
    "pqb": "^0.30.3",
    "reflect-metadata": "^0.2.2",
    "remeda": "^1.61.0"
  },
  "devDependencies": {
    "@graphql-codegen/typescript-graphql-request": "^6.2.0",
    "@types/bcrypt": "^5.0.2",
    "@types/dockerode": "^3.3.29",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.12.12",
    "@types/pg": "^8.11.6",
    "@types/pluralize": "^0.0.33",
    "rake-db": "^2.20.5",
    "tsc-watch": "^6.2.0"
  }
}
