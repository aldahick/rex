{
  "name": "@aldahick/rex-api",
  "version": "1.0.1",
  "description": "Personal API server",
  "main": "dist/main.js",
  "type": "module",
  "scripts": {
    "start": "node --enable-source-maps dist/main",
    "dev": "tsc-watch --onSuccess \"node --enable-source-maps dist/main\"",
    "build": "tsc",
    "lint": "biome check src",
    "generate": "graphql-codegen --config codegen.graphql.ts",
    "db": "tsx src/service/database/database.migration.ts",
    "test": "vitest run"
  },
  "engines": {
    "node": "^20.15"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aldahick/rex.git"
  },
  "author": "Alex Hicks <alex@alexhicks.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/aldahick/rex/issues"
  },
  "homepage": "https://github.com/aldahick/rex#readme",
  "dependencies": {
    "@aldahick/rex-sdk": "workspace:*",
    "@athenajs/core": "^1.2.3",
    "@google-cloud/run": "^1.3.0",
    "axios": "^1.7.2",
    "bcrypt": "^5.1.1",
    "date-fns": "^3.6.0",
    "google-auth-library": "^9.11.0",
    "graphql": "^16.9.0",
    "graphql-request": "6.1.0",
    "ioredis": "^5.4.1",
    "jira.js": "^4.0.1",
    "jsonwebtoken": "^9.0.2",
    "mime": "^4.0.3",
    "orchid-orm": "^1.32.6",
    "pg": "^8.12.0",
    "pluralize": "^8.0.0",
    "pqb": "^0.36.5",
    "reflect-metadata": "^0.2.2",
    "remeda": "^2.2.1"
  },
  "devDependencies": {
    "@graphql-codegen/typescript-graphql-request": "^6.2.0",
    "@swc/core": "^1.6.6",
    "@types/bcrypt": "^5.0.2",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.14.9",
    "@types/pg": "^8.11.6",
    "@types/pluralize": "^0.0.33",
    "@vitest/coverage-v8": "^1.6.0",
    "rake-db": "^2.22.6",
    "tsc-watch": "^6.2.0",
    "unplugin-swc": "^1.4.5",
    "vitest": "^1.6.0"
  }
}
